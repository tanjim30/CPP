#include<iostream>
using namespace std;
int main()
{
  float a,b;
  char op;
  cout<<"***CALCULATOR***"<<endl; 
  cin>>a;
  cin>>op;
  cin>>b;
  if(op == '+')
  {
    cout<<"Answer="<<a+b;
  }
  if(op == '-')
  {
    cout<<"Answer="<<a-b;
  }
  if(op == '*')
    {
      cout<<"Answer="<<a*b;
    }
  if(op == '/')
    {
      cout<<"Answer="<<a/b;
    }
}
